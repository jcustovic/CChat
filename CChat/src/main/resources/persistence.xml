<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
	version="2.0">
    
	<persistence-unit name="cchatContent">
		<class>hr.chus.cchat.model.db.jpa.Operator</class>
		<class>hr.chus.cchat.model.db.jpa.Role</class>
		<class>hr.chus.cchat.model.db.jpa.Configuration</class>
		<class>hr.chus.cchat.model.db.jpa.Nick</class>
		<class>hr.chus.cchat.model.db.jpa.User</class>
		<class>hr.chus.cchat.model.db.jpa.SMSMessage</class>
		<class>hr.chus.cchat.model.db.jpa.Picture</class>
		<class>hr.chus.cchat.model.db.jpa.ServiceProvider</class>
		<class>hr.chus.cchat.model.db.jpa.ServiceProviderKeyword</class>
		<class>hr.chus.cchat.model.db.jpa.KeywordConfiguration</class>
		
		<class>hr.chus.programd.multiplexor.jpa.Predicate</class>
		<class>hr.chus.programd.multiplexor.jpa.PredicateUser</class>
		
		<exclude-unlisted-classes>true</exclude-unlisted-classes>
	</persistence-unit>
	
</persistence>